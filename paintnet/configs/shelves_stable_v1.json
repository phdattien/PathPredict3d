{
	"dataset": "shelves-v1",
	"model": {
		"affinetrans": false,
		"hidden_size": [1024, 1024]
	},
	"pc_points": 5120,
	"traj_points": 4000,
	"lambda_points": 4,
	"overlapping": 1,
	"loss": ["chamfer", "rich_attraction_chamfer"],
	"epochs": 1250,
