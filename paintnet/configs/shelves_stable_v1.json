	"weight_rich_attraction_chamfer": 0.5,
	"batch_size": 32,
	"backbone": "pointnet2",
	"pretrained": true,
	"normalization": "per-dataset",
	"extra_data": ["orientnorm"],
	"weight_orient": 0.25,
	"augmentations": []
}